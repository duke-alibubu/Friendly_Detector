<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#ABFFFF00" />
            <gradient android:angle="-90" android:startColor="#cc9900" android:endColor="#ff9900"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#ABFFFF00" />
            <solid android:color="#D8FFCC00"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#ffcc00" />
            <gradient android:angle="-90" android:startColor="#ABFFFF00" android:endColor="#D8FFCC00" />
        </shape>
    </item>
</selector>