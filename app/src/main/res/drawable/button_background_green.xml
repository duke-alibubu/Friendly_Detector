<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#66ff66" />
            <gradient android:angle="-90" android:startColor="#006600" android:endColor="#00ff00"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#66ff66" />
            <solid android:color="#66ff33"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#66ff66" />
            <gradient android:angle="-90" android:startColor="#66ff33" android:endColor="#00cc00" />
        </shape>
    </item>
</selector>